{{#each erros}}
  <div class="alert alert-danger"> {{texto}} </div>
{{else}}
  
{{/each}}

<h3> Editar Parto</h3>

  <div class="card">
    <div class="card-body">
          <form action="/Parto/addAltParto" method='POST'>
              <p> 

              <input type='hidden' id='id' name='id' value="{{Parto.id}}">

              <label for='nome'> Nome: </label>
                  <input type='text' id='nome' name='nome' value= '{{Parto.nome}}' class='form-control'>

              <label for='slug'> Slug: </label>
                  <input type='text' id='slug' name='slug' value= '{{Parto.slug}}' class='form-control'> 

              <br>
              <button type='submit' class='btn btn-success'>Salvar Parto</button>

          </form>
    </div>  
  </div>